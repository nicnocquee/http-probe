{
  "interval": 20,
  "probes": [
    {
      "id": "1",
      "name": "something",
      "description": "some description",
      "request": {
        "method": "POST",
        "url": "https://something/login",
        "headers": {
          "Authorization": ""
        },
        "body": {
          "username": "someusername",
          "password": "somepassword"
        }
      },
      "alerts": ["status-not-200", "response-greater-than-200"]
    }
  ]
}
